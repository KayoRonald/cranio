{
  "compilerOptions": {
    "outDir": "./dist",
    "allowJs": true,
    "target": "es5",
    "noImplicitAny": false,
    "esModuleInterop": true,
		"declaration": true,
		"paths": {
			"@@/*": ["./src/*"],
			"@config/*": ["./src/config/*"],
			"@helpers": ["./src/helpers/index.ts"],
			"@helpers/*": ["./src/helpers/*"],
			"@models/*": ["./src/models/*"],
			"@dialogflow/*": ["./src/modules/dialogflow/*"],
			"@modules/*": ["./src/modules/*"]
		}
  },
  "include": ["./src/**/*"]
}